{
 gotoAndStop(2);
}

import flash.events.MouseEvent;

stop();

leftButton.addEventListener(MouseEvent.CLICK, goFrame2);

import flash.events.MouseEvent;

stop();

leftButton.addEventListener(MouseEvent.CLICK, goFrame2);
upButton.addEventListener(MouseEvent.CLICK, goFrame5);

function goFrame5(e:MouseEvent):void
{
 gotoAndStop(2);
}

import flash.events.MouseEvent;

stop();

if(MovieClip(root).getRope)
 rope.visible = false;
 
rope.addEventListener(MouseEvent.CLICK, getRopeAction);

function getRopeAction(e:MouseEvent):void
{
 MovieClip(root).getRope = true;
 rope.visible = false;
}

import flash.events.MouseEvent;

stop();

hook.addEventListener(MouseEvent.CLICK, tryEscape);
rightButton.addEventListener(MouseEvent.CLICK, goFrame5);

function tryEscape(e:MouseEvent):void
{
 if(MovieClip(root).getRope)
 {
  gotoAndStop(7);
 }
}
