import flash.events.MouseEvent;

stop();

startButton.addEventListener(MouseEvent.CLICK, startAction);

function startAction(e:MouseEvent):void
{
 gotoAndStop(2);
}

import flash.events.MouseEvent;

stop();

leftButton.addEventListener(MouseEvent.CLICK, leftButtonAction);
rightButton.addEventListener(MouseEvent.CLICK, rightButtonAction);

function leftButtonAction(e:MouseEvent):void
{
 gotoAndStop(3);
}

function rightButtonAction(e:MouseEvent):void
{
 gotoAndStop(4);
}

import flash.events.MouseEvent;

stop();

